{
  "eslint.enable": true, //是否开启vscode的eslint
  //    "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.options": {
    //指定vscode的eslint所处理的文件的后缀
    "extensions": [".js", ".vue", ".ts", ".tsx"]
  },
  "eslint.validate": [
    //确定校验准则
    "javascript",
    "javascriptreact"
  ]
}
