    "moment": "^2.29.1",
    "react": "17.x",
    "react-dom": "17.x",
    "shengjs": "^2.0.6",
    "styled-components": "^5.3.3",
    "umi": "^3.5.20",
    "use-immer": "^0.6.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.20",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "@umijs/preset-react": "^1.8.30",
